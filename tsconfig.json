{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "resources/js/*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "resources/js/**/*",
    "resources/js/**/*.vue",
    "resources/js/**/*.d.ts"
  ]
}
